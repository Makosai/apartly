{
  "name": "apartly",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "supa:gen": "npx supabase gen types --lang=typescript --schema public --project-id \"wqyqougnnqwgqhjshpvz\" > ./src/lib/db/types/supabase.types.ts",
    "supa:link": "npx supabase link --project-ref \"wqyqougnnqwgqhjshpvz\"",
    "supa:pull": "npx supabase db pull",
    "supa:push": "npx supabase db push"
  },
  "devDependencies": {
    "@iconify/svelte": "^4.0.2",
    "@playwright/test": "^1.28.1",
    "@skeletonlabs/skeleton": "2.10.2",
    "@skeletonlabs/tw-plugin": "0.4.0",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/adapter-node": "^5.2.2",
    "@sveltejs/adapter-vercel": "^5.4.4",
    "@sveltejs/enhanced-img": "^0.3.7",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@types/eslint": "8.56.0",
    "@types/leaflet": "^1.9.12",
    "@types/node": "22.5.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "10.4.20",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "8.4.45",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "3.4.11",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-tailwind-purgecss": "0.3.3",
    "vitest": "^1.2.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/inter": "^5.1.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.0.0"
  }
}
